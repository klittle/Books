<h2>List of Users</h2>

<table>
  <tr>
    <th>Logins</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.login %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<p><%= link_to 'Is Your Name Here? If so, then Log In', new_session_path %></p>
<%= link_to 'If Not, Add Your Name', new_user_path %>